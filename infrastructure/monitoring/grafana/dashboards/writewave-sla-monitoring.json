{
  "dashboard": {
    "id": null,
    "title": "WriteWave SLA Monitoring",
    "tags": ["writewave", "sla", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-24h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Overall SLA Status",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(rate(http_requests_total{status=~\"2..\"}[5m]) / rate(http_requests_total[5m])) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Response Time SLA",
        "type": "stat",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th Percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 1
                },
                {
                  "color": "red",
                  "value": 2
                }
              ]
            },
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Uptime SLA",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(up{job=~\"user-service|content-service|progress-service|community-service|notification-service|analytics-service\"}) * 100",
            "legendFormat": "Uptime %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 95
                },
                {
                  "color": "green",
                  "value": 99.9
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Error Rate SLA",
        "type": "stat",
        "targets": [
          {
            "expr": "avg(rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m])) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": 0
                },
                {
                  "color": "yellow",
                  "value": 0.1
                },
                {
                  "color": "red",
                  "value": 1
                }
              ]
            },
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 6,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Service Availability",
        "type": "graph",
        "targets": [
          {
            "expr": "up{job=~\"user-service|content-service|progress-service|community-service|notification-service|analytics-service\"}",
            "legendFormat": "{{job}}"
          }
        ],
        "yAxes": [
          {
            "label": "Status",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 6
        }
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 6
        }
      },
      {
        "id": 7,
        "title": "Success Rate by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"2..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "{{job}}"
          }
        ],
        "yAxes": [
          {
            "label": "Success Rate %",
            "min": 0,
            "max": 100
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 14
        }
      },
      {
        "id": 8,
        "title": "Error Rate by Service",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{status=~\"5..\"}[5m]) / rate(http_requests_total[5m]) * 100",
            "legendFormat": "{{job}}"
          }
        ],
        "yAxes": [
          {
            "label": "Error Rate %",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 14
        }
      },
      {
        "id": 9,
        "title": "SLA Breach Events",
        "type": "graph",
        "targets": [
          {
            "expr": "ALERTS{alertname=~\"SLABreach|ResponseTimeSLABreach\"}",
            "legendFormat": "{{alertname}}"
          }
        ],
        "yAxes": [
          {
            "label": "Breach Events",
            "min": 0
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 22
        }
      },
      {
        "id": 10,
        "title": "Database SLA",
        "type": "graph",
        "targets": [
          {
            "expr": "up{job=\"postgres-exporter\"}",
            "legendFormat": "PostgreSQL"
          },
          {
            "expr": "up{job=\"redis-exporter\"}",
            "legendFormat": "Redis"
          }
        ],
        "yAxes": [
          {
            "label": "Status",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 22
        }
      },
      {
        "id": 11,
        "title": "Message Queue SLA",
        "type": "graph",
        "targets": [
          {
            "expr": "up{job=\"rabbitmq-exporter\"}",
            "legendFormat": "RabbitMQ"
          },
          {
            "expr": "up{job=\"kafka-exporter\"}",
            "legendFormat": "Kafka"
          }
        ],
        "yAxes": [
          {
            "label": "Status",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 30
        }
      },
      {
        "id": 12,
        "title": "SLA Compliance Summary",
        "type": "table",
        "targets": [
          {
            "expr": "avg_over_time(up{job=~\"user-service|content-service|progress-service|community-service|notification-service|analytics-service\"}[24h]) * 100",
            "legendFormat": "{{job}}"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Uptime %",
                "job": "Service"
              }
            }
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 30
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(up{job=~\"user-service|content-service|progress-service|community-service|notification-service|analytics-service\"}, job)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "sla_threshold",
          "type": "custom",
          "query": "99,99.5,99.9,99.95,99.99",
          "current": {
            "text": "99.9",
            "value": "99.9"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "SLA Breaches",
          "datasource": "Prometheus",
          "expr": "ALERTS{alertname=~\"SLABreach|ResponseTimeSLABreach\"}",
          "iconColor": "red",
          "enable": true
        },
        {
          "name": "Service Deployments",
          "datasource": "Prometheus",
          "expr": "changes(up[1m]) > 0",
          "iconColor": "blue",
          "enable": true
        }
      ]
    }
  }
}
